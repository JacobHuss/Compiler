MODULE Bubble ;

TYPE
    ListType = ARRAY [ 1 .. 10 ] OF INTEGER ;

VAR
    thisList : ListType ;

(********************************************************************)
(****                 F I L L                                    ****)
(********************************************************************)
PROCEDURE Fill ( VAR list : ListType ) ;

BEGIN
    list[1] := 42;
END Fill ;

(********************************************************************)
(****                 M A I N                                    ****)
(********************************************************************)
BEGIN
    Fill  ( thisList ) ;
END Bubble.